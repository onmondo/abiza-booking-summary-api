{
  "name": "abiza-booking-summary-api",
  "version": "1.0.0",
  "description": "A local web-app that monitors expenses and earnings from guest bookings.",
  "main": "index.js",
  "scripts": {
    "seed": "ts-node-dev src/seeder/index.ts",
    "dev": "ts-node-dev --poll src/app.ts",
    "dev:debug": "NODE_OPTIONS=\"--inspect=0.0.0.0:9230\" ts-node-dev --poll src/app.ts",
    "build": "rimraf ./dist && tsc",
    "start:local": "npm run build && node dist/app.js",
    "start:debug": "npm run build && node --inspect=localhost:9230 dist/app.js",
    "start": "npm run build && pm2 start dist/app.js --watch 'src/*.ts'"
  },
  "author": "John Raymond Blando",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "10.45.2",
    "amqplib": "0.10.3",
    "bcrypt": "5.1.1",
    "big.js": "6.2.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "csvtojson": "2.0.10",
    "express": "4.18.3",
    "express-rate-limit": "7.2.0",
    "helmet": "7.1.0",
    "joi": "17.12.2",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "mongoose": "8.2.1",
    "otplib": "12.0.1",
    "pug": "3.0.2",
    "uuid": "9.0.1",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/amqplib": "0.10.5",
    "@types/bcrypt": "5.0.2",
    "@types/big.js": "6.2.2",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/csvtojson": "2.0.0",
    "@types/express": "4.17.21",
    "@types/express-rate-limit": "6.0.0",
    "@types/helmet": "4.0.0",
    "@types/jsonwebtoken": "9.0.6",
    "@types/lodash": "4.14.202",
    "@types/pug": "2.0.10",
    "@types/uuid": "9.0.8",
    "@types/ws": "8.5.10",
    "dotenv": "16.4.5",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2"
  }
}